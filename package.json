{
  "name": "pied_finder",
  "version": "1.0.0",
  "description": "An easy to use Web App, making it simple to search for episodes of the hit HBO tv-show Silicon Valley.",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently --kill-others \"npm run json:server\" \"npm run watch\"",
    "json:server": "json-server --watch data/db.json",
    "watch": "browser-sync start --server"
  },
  "keywords": [
    "CRUD",
    "vue.js",
    "Silicon",
    "Valley",
    "Search"
  ],
  "author": "Abraham Sedri",
  "license": "ISC",
  "dependencies": {
    "angular": "^1.5.0",
    "angular-animate": "^1.5.0",
    "angular-aria": "^1.5.0",
    "angular-material": "^0.11.4",
    "json-server": "^0.12.1",
    "lodash": "^4.17.4",
    "mdi": "^1.4.57"
  },
  "devDependencies": {
    "angular-mocks": "^1.6.7",
    "browser-sync": "^2.18.13",
    "gulp": "^3.9.1",
    "gulp-json-srv": "^1.1.0",
    "jasmine": "^2.8.0",
    "jasmine-core": "^2.8.0",
    "karma": "^1.7.1",
    "karma-jasmine": "^1.1.0"
  }
}
